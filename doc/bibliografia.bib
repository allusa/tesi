@article{stonebraker10,
 author = {Stonebraker, Michael},
 title = {SQL databases v. NoSQL databases},
 journal = {Communications of the ACM},
 issue_date = {April 2010},
 volume = {53},
 number = {4},
 month = {04},
 year = {2010},
 pages = {10--11},
 doi = {10.1145/1721654.1721659},
 publisher = {ACM},
 address = {New York, NY, USA},
 annotation = {Stonebraker comenta que els productes NoSQL exploren la part de baix nivell dels SGBD. Explica que el canvi entre diferents SGBD es pot donar per motius de rendiment i flexibilitat i parla dels criteris de rendiment (OLTP, logging, buffer, ACID, etc.). Conclou que es pot parlar d'aquests criteris però que no s'hauria d'abandonar l'SQL.},
} 

@article{stonebraker09,
 author = {Stonebraker, Michael and Hong, Jason},
 title = {Saying good-bye to DBMSs, designing effective interfaces},
 journal = {Communications of the ACM},
 issue_date = {September 2009},
 volume = {52},
 number = {9},
 month = {09},
 year = {2009},
 pages = {12--13},
 doi = {10.1145/1562164.1562169},
 publisher = {ACM},
 address = {New York, NY, USA},
 annotation = {Stonebraker descriu la millora de rendiment de les noves implementacions de SGBD, però confón en situar les implmentacions tradicionals de SGBD com a model relacional. Diu que les implementacions amb taules poden no ser adients en determinades aplicacions, i té raó, però no és cert que el model relacional obligui a que la implementació es faci amb relacions sinó que es pot fer amb qualsevol dels mètodes que descriu.} 
}

@inproceedings{stonebraker07,
 author = {Stonebraker, Michael and Madden, Samuel and Abadi, Daniel J. and Harizopoulos, Stavros and Hachem, Nabil and Helland, Pat},
 title = {The end of an architectural era: (it's time for a complete rewrite)},
 booktitle = {Proceedings of the 33rd international conference on Very large data bases},
 series = {VLDB'07},
 year = {2007},
 location = {Vienna, Austria},
 pages = {1150--1160},
 url = {http://www.vldb.org/conf/2007/papers/industrial/p1150-stonebraker.pdf},
 urldate = {2012-03-26},
 publisher = {VLDB Endowment},
 annotation = {Stonebraker intueix la fi dels SGBD 'one size fits all' pel que afa a rendiment, és a dir que creu que cada aplicació necessita una implementació adequada. Amb aquesta afirmació té tota la raó, en dóna mostres amb detalls d'implementació que milloren el rendiment, però al final confón model amb implementació: 'The Relational Model Is not Necessarily the Answer'. Tot i que reconeix que 'stars and snowflakes are easily modeled using relational schemas' confón en atribuir problemes de rendiment al model relacional, ja que aquest com a model no té i no pot tenir cap especificació de rendiment. Tot i que més endavant atribueix la culpa correctament a les implementacions: 'SQL is Not the Answer'.},
} 


